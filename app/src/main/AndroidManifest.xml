<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NowPlayingActivity"
            android:label="@string/title_activity_now_playing"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SongListActivity"
            android:label="@string/title_activity_song_list"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PlaylistActivity"
            android:label="@string/title_activity_playlist"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MusicStoreActivity"
            android:label="@string/title_activity_music_store"
            android:parentActivityName=".MainActivity">

        </activity>
    </application>

</manifest>